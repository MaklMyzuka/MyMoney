<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="sql_update_transaction">
        UPDATE [transaction]
        SET
            [category_id] = ?2,
            [account_id] = ?3,
            [sum10000] = ?4,
            [product] = ?5,
            [created_at_utc] = ?6
        WHERE [transaction].[id] = ?1;
    </string>
    <string name="sql_update_transfer">
        UPDATE [transfer]
        SET
            [account_id_from] = ?2,
            [account_id_to] = ?3,
            [sum10000_from] = ?4,
            [sum10000_to] = ?5,
            [created_at_utc] = ?6
        WHERE [transfer].[id] = ?1;
    </string>
    <string name="sql_update_currency">
        UPDATE [currency]
        SET
            [name] = ?2,
            [symbol] = ?3,
            [is_visible] = ?4,
            [comment] = ?5
        WHERE [currency].[id] = ?1;
    </string>
    <string name="sql_update_account">
        UPDATE [account]
        SET
            [name] = ?2,
            [currency_id] = ?3,
            [sum10000] = ?4,
            [is_visible] = ?5,
            [comment] = ?6
        WHERE [account].[id] = ?1;
    </string>
    <string name="sql_update_category">
        UPDATE [category]
        SET
            [name] = ?2,
