            [is_visible] = ?4,
            [comment] = ?5
        WHERE [category].[id] = ?1;
    </string>
    <string name="sql_update_account_group">
        UPDATE [account_group]
        SET
            [name] = ?2,
            [is_visible] = ?3,
            [comment] = ?4
        WHERE [account_group].[id] = ?1;
    </string>
</resources>